 <?xml version="1.0" encoding="UTF-8"?> <!DOCTYPE
         generatorConfiguration
                 PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
                 "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd"> <generatorConfiguration>
        <!-- 数据库驱动:选择你的本地硬盘上面的数据库驱动包!!!-->
    >     <classPathEntry  location="D:\myRepository\mysql\mysql-connector-java\8.0.11\mysql-connector-java-8.0.11.jar"/>
    >     <context id="DB2Tables"  targetRuntime="MyBatis3Simple">
    >         <commentGenerator>
    >             <property name="suppressDate" value="true"/>
    >             <!-- 是否去除自动生成的注释 true：是 ： false:否 -->
    >             <property name="suppressAllComments" value="true"/>
    >         </commentGenerator>
    >         <!--数据库链接URL，用户名、密码 -->
    >         <jdbcConnection driverClass="com.mysql.cj.jdbc.Driver" connectionURL="" userId="" password="">
    >         </jdbcConnection>
    >         <javaTypeResolver>
    >             <property name="forceBigDecimals" value="false"/>
    >         </javaTypeResolver>
    >
    >         <!-- 生成模型的包名和位置-->
    >         <javaModelGenerator targetPackage="com.XXXX.model" targetProject="src/main/java">
    >             <property name="enableSubPackages" value="true"/>
    >             <property name="trimStrings" value="true"/>
    >         </javaModelGenerator>
    >
    >           <!--生成service,serviceImpl-->
    <javaServiceGenerator targetPackage="com.design.service" targetProject="src/main/java">
        <property name="enableSubPackages" value="true"/>
    </javaServiceGenerator>

    <!--生成controller-->
    <javaControllerGenerator targetPackage="com.design.controller" targetProject="src/main/java">
        <property name="enableSubPackages" value="true"/>
    </javaControllerGenerator>

    >         <!-- 生成映射文件的包名和位置-->
    >         <sqlMapGenerator  targetPackage="resources.mybatis.mapper" targetProject="src/main">
    >             <property name="enableSubPackages" value="true"/>
    >         </sqlMapGenerator>
    >
    >         <!-- 生成DAO的包名和位置-->
    >         <javaClientGenerator type="XMLMAPPER"  targetPackage="c" targetProject="src/main/java">
    >             <property name="enableSubPackages" value="true"/>
    >         </javaClientGenerator>
    >         <!-- 要生成的表 tableName是数据库中的表名或视图名
>         domainObjectName是实体类名
>         修改四处，表名、对象名、排序字段、主键ID(可选)
>         -->
    >         <table tableName="表名"
>                domainObjectName="对象名" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false"
    > enableSelectByExample="false" selectByExampleQueryId="false">
    >             <property name="selectAllOrderByClause" value="createDate  desc"/>
    >             <property name="useActualColumnNames" value="true"/>
    >             <!--<generatedKey column="parameterValueId" sqlStatement="Mysql" identity="true" />-->
    >         </table>
    >         <!--自增主键可不使用此属性<generatedKey column="tokenId" sqlStatement="Mysql" identity="true" />-->
    >     </context> </generatorConfiguration>
